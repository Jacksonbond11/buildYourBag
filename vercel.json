{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/add-disc.js", "use": "@vercel/node" },
    { "src": "api/createAccount.js", "use": "@vercel/node" },
    { "src": "api/discdata.js", "use": "@vercel/node" },
    { "src": "api/get-average-speed.js", "use": "@vercel/node" },
    { "src": "api/get-category-count.js", "use": "@vercel/node" },
    { "src": "api/get-discs.js", "use": "@vercel/node" },
    { "src": "api/login.js", "use": "@vercel/node" },
    { "src": "api/remove-disc.js", "use": "@vercel/node" },
    { "src": "api/search.js", "use": "@vercel/node" }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    } 
  ],
  {
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
}
